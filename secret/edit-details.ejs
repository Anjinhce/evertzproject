<%- include ('../secret/heade') %>
      <!-- Sidebar Navigation end-->
      <div class="page-content">
        <div class="page-header">
          <div class="container-fluid">
            <h2 class="h5 no-margin-bottom">Dashboard</h2>
          </div>
        </div>
        <section class="no-padding-bottom">
          <div class="container-fluid">
            <div class="row">
              <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <% for(var i=0; i < my_data.length; i++) { %>
                  <form method="POST" action="modifyDetails/<%= my_data[i].ID %>">
                  <div class="wrapper1">
                      <br>
                      <h2 class="form-heading">Enter Employee Payroll Details</h2>
                      <hr class="hr-divider">
           
                      <div class="row">
                        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                          <div class="material-textfield blue" style="margin-top: 30px;">
                            <input type="text" name="employee_id" value="<%= my_data[i].EMP_ID %>" required/>
                            <label data-content="Employee ID">Employee ID</label>
                          </div>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                          <div class="material-textfield red" style="margin-top: 30px;">
                            <input type="text" name="basic_salary" value="<%= my_data[i].BASIC_SALARY %>" required/>
                            <label data-content="Basic Salary">Basic Salary</label>
                         </div>
                        </div>
                      </div>

                      <div class="row">
                        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                          <div class="material-textfield blue">
                            <input type="text" name="hra" value="<%= my_data[i].HRA %>" required/>
                            <label data-content="HRA">HRA</label>
                          </div>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                          <div class="material-textfield red">
                            <input type="text" name="sa" value="<%= my_data[i].SA %>" required/>
                            <label data-content="SA">SA</label>
                         </div>
                        </div>
                      </div>
        
                      <div class="row">
                        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                          <div class="material-textfield blue">
                            <input type="text" name="professional_tax" value="<%= my_data[i].PROFESSIONA_TAX %>" required/>
                            <label data-content="Professional TAX(in %)">Professional TAX(in %)</label>
                          </div>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                          <div class="material-textfield red">
                            <input type="text" name="tax_source" value="<%= my_data[i].TAX_DETUCTION_FROM_SORCE %>" required/>
                            <label data-content="Tax Deductions from source">Tax Deductions from source</label>
                         </div>
                        </div>
                      </div>
        
                      <div class="row">
                        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                          <div class="material-textfield blue">
                            <input type="text" name="standard_deduction" value="<%= my_data[i].STANDARD_DETUCTION %>" required/>
                            <label data-content="Standard Deduction">Standard Deduction</label>
                          </div>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                          <div class="material-textfield red">
                            <input type="text" name="other_deductions" value="<%= my_data[i].OTHERS_DETUCTION %>" required/>
                            <label data-content="Other Deductions">Other Deductions</label>
                         </div>
                        </div>
                      </div>

                      <div class="row">
                        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                          <div class="material-textfield blue">
                            <input type="text" name="net_amount" value="<%= my_data[i].NET_AMOUNT %>" required/>
                            <label data-content="Net Amount">Net Amount</label>
                          </div>
                        </div>
                      </div>
        <br>
        <button class="btn-submit">Submit</button>
      </div>
      </form>
      <% } %>
              </div>
            </div>
          </div>
        </section>
      </div>
    </div>
    <script src="http://localhost:1915/vendor/jquery/jquery.min.js"></script>
    <script src="http://localhost:1915/vendor/popper.js/umd/popper.min.js"> </script>
    <script src="http://localhost:1915/vendor/bootstrap/js/bootstrap.min.js"></script>
    <script src="http://localhost:1915/vendor/jquery.cookie/jquery.cookie.js"> </script>
    <script src="http://localhost:1915/vendor/chart.js/Chart.min.js"></script>
    <script src="http://localhost:1915/vendor/jquery-validation/jquery.validate.min.js"></script>
    <script src="http://localhost:1915/js/charts-home.js"></script>
    <script src="http://localhost:1915/js/front.js"></script>
  </body>
</html>