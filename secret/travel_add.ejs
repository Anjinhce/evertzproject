    <script>
    $("#emp_id").focusout(function()
    {
      $.ajax({
        type:'get',
        data: {send_data: $("#emp_id").val()},
        url: '/fetch_id',
        success:function(newData)
        {
          if(newData=="Employee doesn't exist")
          {
            $("#empid-label").html(newData)
            $("#emp_name").val("");
          }
          else
          {
            $("#emp_name").val(newData)
            $("#empid-label").html("");
          }
        }
      })
    })
    </script>
