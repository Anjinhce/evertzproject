                                  <% }) %>
                              </select>
                        </div>
                      </div>
                      <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                        <div class="material-textfield red">
                            <select name="customer_id" class="custom-select my-1 mr-sm-2" id="inlineFormCustomSelectPref">
